[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red
  new = cyan
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  excludesfile = .gitignore.global
  editor = vim
[merge]
  tool = vimdiff
[include]
  path = .gitconfig.user
